{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "all",
  "insertFinalNewline": true,
  "experimentalSortPackageJson": true,
  "experimentalSortImports": {
    "order": "asc",
    "newlinesBetween": true,
    "internalPattern": ["@/"]
  },
  "ignorePatterns": ["node_modules/"]
}
